                    <% }); %>
                    <div>
                        <input class="forminp" type="hidden" name="result" value='<%- JSON.stringify(col) %>'>
                    </div>
                    <div id="divbtn">
                        <input type="submit" id="subbtn" value="Save">
                    </div>
                </div>
            </form>
        </div>
    </div>
