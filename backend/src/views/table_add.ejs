    <div id="body">
        <div class="cont">
            <form action="/admin/<%= table %>/add" method="post">
                <div id="filt">
                    <% col.forEach((c)=>{ %>
                        <div class="filtinp">
                            <%= c %>
                            <input class="forminp" name="<%= c %>" required value="">
